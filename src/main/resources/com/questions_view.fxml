<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.QuestionsViewController">
    <children>
        <TableView fx:id="questionsTable" layoutX="14.0" layoutY="14.0" prefHeight="400.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="questionTextColumn" prefWidth="300.0" text="Question Text" />
                <TableColumn fx:id="categoryColumn" prefWidth="200.0" text="Category" />
            </columns>
        </TableView>
    </children>
</AnchorPane>