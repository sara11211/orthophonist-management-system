<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.TestOptionsController">
    <Text layoutX="120.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create Test">
        <font>
            <Font name="System Bold" size="20.0" />
        </font>
    </Text>
    <Button fx:id="createTestQuestionnaireButton" layoutX="100.0" layoutY="70.0" mnemonicParsing="false" onAction="#handleCreateTestQuestionnaire" text="Create Test Questionnaire" />
    <Button fx:id="createTestExerciceButton" layoutX="100.0" layoutY="110.0" mnemonicParsing="false" onAction="#handleCreateTestExercice" text="Create Test Exercice" />
</AnchorPane>
