<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ViewTestsController">
    <TableView fx:id="testsTable" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="400.0">
        <columns>
            <TableColumn fx:id="testNameColumn" prefWidth="200.0" text="Test Name">
                <cellValueFactory>
                    <PropertyValueFactory property="nom" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="testDescriptionColumn" prefWidth="200.0" text="Description">
                <cellValueFactory>
                    <PropertyValueFactory property="capacite" />
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>
</AnchorPane>
