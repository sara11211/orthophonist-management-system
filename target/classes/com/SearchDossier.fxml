<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<BorderPane fx:id="borderPane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.RechercheDossierController">
    <center>
        <GridPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171">
            <padding>
                <Insets top="10" right="10" bottom="10" left="10"/>
            </padding>
            <hgap>10</hgap>
            <vgap>10</vgap>


            <VBox spacing="10" alignment="CENTER" >
                <children>
                    <TextField fx:id="searchField" promptText="Enter patient ID" />
                    <Button text="Submit" onAction="#handleSearchButtonAction" />
                </children>
            </VBox>
        </GridPane>
    </center>
</BorderPane>
